#include<stdio.h>
void main() {
    int arr[5],i,high,count;
    printf("Input 5 integers: \n");
    for(i=0;i<5;i++){
        scanf("%d", &arr[i]);
        if(arr[i]>high){
            high=arr[i];
            count=i+1;
        }
    }
    printf("Highest Value: %d\n", high);
    printf("Position: %d\n", count);
}
