#include <stdio.h>

int main() {
   int n,i,s=0;
   printf("Enter the size of an array: ");
   scanf("%d",&n);
   int ar[n];
   printf("Enter the elements: ");
   for(i=0;i<n;i++){
       scanf("%d",&ar[i]);
   }
for(i=0;i<n;i++){
    if(ar[i]<0){
       s = s+ar[i]; 
    }
}
printf("sum: %d",s);

    return 0;
}
