#include <stdio.h>

int main() {
   int n;
    printf("Enter the size of array: ");
    scanf("%d",&n);
    int ar[n],i,p=0;
    printf("Enter the elements: ");
    for(i=0;i<n;i++){
     scanf("%d",&ar[i]);
    }
for(i=0;i<n;i++){
    if(ar[i]<0){
       ar[i]=0;
      
    }
     printf("%d ",ar[i]);
}
    return 0;
}
