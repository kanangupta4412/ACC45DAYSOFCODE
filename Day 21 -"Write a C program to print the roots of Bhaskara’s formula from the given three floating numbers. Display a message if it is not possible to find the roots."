#include<stdio.h>
#include<math.h>
void main() {
    float a,b,c;
    float root1, root2, D;
    printf("Input the first number(a): ");
    scanf("%f", &a);
    printf("Input the second number(b): ");
    scanf("%f", &b);
    printf("Input the third number(c): ");
    scanf("%f", &c);
    D = (b*b)-(4*a*c);
    if(D>0||D==0){
        root1 = (-b + sqrt(D))/(2*a);
        root2 = (-b - sqrt(D))/(2*a);
        printf("Root1 = %f\n", root1);
        printf("Root2 = %f\n", root2);
    }
    else{
        printf("Roots of these variables is not possible..Please try with other variables..");
    }
}
